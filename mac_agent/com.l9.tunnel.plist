<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.l9.tunnel</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/opt/l9_agent/reverse_tunnel.sh</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/opt/l9_agent/logs/tunnel.log</string>
    
    <key>StandardErrorPath</key>
    <string>/opt/l9_agent/logs/tunnel.error.log</string>
    
    <key>WorkingDirectory</key>
    <string>/opt/l9_agent</string>
</dict>
</plist>

